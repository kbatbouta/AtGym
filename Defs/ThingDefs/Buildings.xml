<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ThingDef ParentName="BenchBase">
        <defName>FitnessLiftingBench</defName>
        <label>Lifting Bench</label>
        <description>A Lifting Bench for those wanting to pump some steel!</description>
            <thingClass>Building_WorkTable</thingClass>
        <graphicData>
            <texPath>Things/Buildings/Lifting_bench_south</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(2.3,2)</drawSize>
            <shadowData>
                <volume>(0.3, 0.3, 1.3)</volume>
                <offset>(0, 0, -0.2)</offset>
            </shadowData>
        </graphicData>        
        <researchPrerequisites>
        </researchPrerequisites>
        <size>(1,2)</size>
        <rotatable>false</rotatable>
        <altitudeLayer>Building</altitudeLayer>
        <passability>PassThroughOnly</passability>
        <designationCategory>Production</designationCategory>
        <minifiedDef>MinifiedThing</minifiedDef>
        <thingCategories>
            <li>BuildingsMisc</li>
        </thingCategories>
        <canOverlapZones>false</canOverlapZones>
        <hasInteractionCell>true</hasInteractionCell>
        <interactionCellOffset>(0, 0, -1)</interactionCellOffset>
        <surfaceType>Item</surfaceType>
        <statBases>
            <MaxHitPoints>180</MaxHitPoints>
            <WorkToBuild>600</WorkToBuild>
            <Flammability>1.0</Flammability>
            <Cleanliness>-4</Cleanliness>
        </statBases>
        <costList>
            <Steel>50</Steel>
        </costList>
        <inspectorTabs/>
        <comps>
            <li Class="CompProperties_AffectedByFacilities">
                <linkableFacilities />
            </li>
            <li Class="CompProperties_Breakdownable"/>
        </comps>
        <constructEffect>ConstructMetal</constructEffect>
        <constructionSkillPrerequisite>0</constructionSkillPrerequisite>
        <placeWorkers>
            <li>PlaceWorker_PreventInteractionSpotOverlap</li>
        </placeWorkers>
    </ThingDef>
</Defs>
